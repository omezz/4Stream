		</activity>
		<activity android:name="com.android.a4sp.play.PlaySound"></activity>
		<activity android:name=".A4SPActivity"></activity>

	</application>
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.WAKE_LOCK" />
	
</manifest>